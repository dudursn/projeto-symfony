{% set template = template is defined?template:'default' %}
{% if template == 'default' %}
    <script type="text/template" id="fineuploader-template-default">
        <div class="qq-uploader-selector qq-uploader" qq-drop-area-text="Arraste arquivos para esta página" style="max-height: 680px; overflow-y: visible;">
            <div class="qq-upload-drop-area-selector qq-upload-drop-area" qq-hide-dropzone>
                <span class="qq-upload-drop-area-text-selector"></span>
            </div>
            <div class="row" style="margin-bottom: 10px;">
                <div class="col-md-6">
                    <div class="btn-group" role="group" aria-label="...">
                        <a class="qq-upload-button-selector btn btn-primary">
                            <i class="fa fa-search"></i> Selecionar Arquivos
                        </a>
                        <a class="btn btn-default rtlUploaderUploadFiles" title="Envie os arquivos para o sistema sem salvá-los">
                            <i class="fa fa-upload"></i> Enviar
                        </a>
                    </div>
                </div>
                <div class="col-md-6 rtlClearArea">
                    <div class="btn-group pull-right" role="group" aria-label="...">
                        <a class="btn btn-default rtlUploaderCancelAll" title="Cancelar envio dos Arquivos pendentes"><i class="fa fa-ban"></i> </a>
                        <a class="btn btn-default rtlUploaderDeleteAll" title="Excluir Arquivos já enviados"><i class="fa fa-trash"></i> </a>
                        <a class="btn btn-default rtlUploaderClearAll" title="Excluir todos os Arquivos (cancela os pendentes e exclui os arquivos enviados)"><i class="fa fa-times"></i></a>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="qq-total-progress-bar-container-selector progress " style="width: 100%;">
                        <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="qq-total-progress-bar-selector bar qq-progress-bar" style="background-color: green;"></div>
                    </div>
                </div>
            </div>
            <span class="qq-drop-processing-selector qq-drop-processing">
                <span>Processing dropped files...</span>
                <span class="qq-drop-processing-spinner-selector qq-drop-processing-spinner"></span>
            </span>

            <div class="table-responsive">
            <table class="table table-scroll">
                <tbody class="qq-upload-list-selector">
                    <tr>
                        <td><img class="qq-thumbnail-selector" qq-max-size="100" qq-server-scale></td>
                        <td>
                            <div>
                                <strong class="qq-upload-file-selector" style="cursor: pointer;"></strong>
                                <i class="qq-edit-filename-icon-selector qq-edit-filename-icon fa fa-pencil" aria-label="Edit filename" style="margin-left: 5px; background-image: none;"></i>
                                <input class="qq-edit-filename-selector qq-edit-filename" tabindex="0" type="text">                            
                            </div>
                            <div>
                                <div class="qq-progress-bar-container-selector progress" style="width: 150px; margin-bottom: 0px;">
                                    <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="qq-progress-bar-selector bar qq-progress-bar" style="background-color: green;"></div>
                                </div>
                            </div>
                            <div class="text-muted">
                                <i class="qq-upload-spinner-selector fa fa-spinner fa-spin"></i> 
                                <strong role="status" class="qq-upload-status-text-selector"></strong>
                            </div>
                        </td>
                        <td><span class="qq-upload-size-selector qq-upload-size"></span></td>
                        <td class="actions">
                            <button type="button" class="qq-upload-cancel-selector btn btn-default"><i class="fa fa-ban"></i> Cancelar</button>
                            <button type="button" class="qq-upload-retry-selector btn btn-warning"><i class="fa fa-refresh"></i> Reenviar</button>
                            <button type="button" class="qq-upload-delete-selector btn btn-danger"><i class="fa fa-times"></i> Excluir</button>
                        </td>
                    </tr>
                </tbody>
            </table>
            </div>
            <dialog class="qq-alert-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">Close</button>
                </div>
            </dialog>

            <dialog class="qq-confirm-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">No</button>
                    <button type="button" class="qq-ok-button-selector">Yes</button>
                </div>
            </dialog>

            <dialog class="qq-prompt-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <input type="text">
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">Cancel</button>
                    <button type="button" class="qq-ok-button-selector">Ok</button>
                </div>
            </dialog>
        </div>
    </script>
{% elseif template == 'vertical' %}
    <script type="text/template" id="fineuploader-template-vertical">
        <div class="qq-uploader-selector qq-uploader" qq-drop-area-text="Arraste arquivos para esta página" style="max-height: 680px; overflow-y: visible;">
            <div class="qq-upload-drop-area-selector qq-upload-drop-area" qq-hide-dropzone>
                <span class="qq-upload-drop-area-text-selector"></span>
            </div>
            <div class="row" style="margin-bottom: 10px;">
                <div class="col-md-12">
                    <a class="qq-upload-button-selector btn btn-primary btn-block">
                        <i class="fa fa-search"></i> Selecionar Arquivos
                    </a>
                    <a class="btn btn-default rtlUploaderUploadFiles" title="Envie os arquivos para o sistema sem salvá-los" style="margin-top: 5px;">
                        <i class="fa fa-upload"></i> Enviar
                    </a>
                    <div class="btn-group rtlClearArea" style="margin-top: 5px;">
                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-trash"></i> <span class="caret"></span></button>
                        <ul class="dropdown-menu">
                            <li><a class="rtlUploaderCancelAll" href="javascript: " title="Cancelar envio dos Arquivos pendentes"><i class="fa fa-ban"></i> Cancelar Todos</a></li>
                            <li><a class="rtlUploaderDeleteAll" href="javascript: " title="Excluir Arquivos já enviados"><i class="fa fa-trash"></i> Excluir Todos</a></li>
                            <li><a class="rtlUploaderClearAll" href="javascript: " title="Excluir todos os Arquivos (cancela os pendentes e exclui os arquivos enviados)"><i class="fa fa-times"></i> Limpar</a></li>
                        </ul>
                    </div>
                    
                </div>
                
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="qq-total-progress-bar-container-selector progress" style="width: 100%;">
                        <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="qq-total-progress-bar-selector bar qq-progress-bar" style="background-color: green;"></div>
                    </div>
                </div>
            </div>
            <span class="qq-drop-processing-selector qq-drop-processing">
                <span>Processing dropped files...</span>
                <span class="qq-drop-processing-spinner-selector qq-drop-processing-spinner"></span>
            </span>
            
            <ul class="list-group table-scroll qq-upload-list-selector">
                <li class="list-group-item">
                    <div class="text-center">
                        <img class="qq-thumbnail-selector" qq-max-size="100" qq-server-scale>
                    </div>
                    <div class="text-center" style="margin-top: 5px;">
                        <strong class="qq-upload-file-selector" style="cursor: pointer;"></strong>
                        <i class="qq-edit-filename-icon-selector qq-edit-filename-icon fa fa-pencil" aria-label="Edit filename" style="margin-left: 5px; background-image: none;"></i>
                        <input class="qq-edit-filename-selector qq-edit-filename" tabindex="0" type="text">
                        <span class="qq-upload-size-selector qq-upload-size text-muted"></span>
                    </div>
                    <div class="qq-progress-bar-container-selector progress" style="width: 100%; margin-top: 5px; margin-bottom: 0px;">
                        <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="qq-progress-bar-selector bar qq-progress-bar" style="background-color: green;"></div>
                    </div>
                    <div class="text-muted text-center" style="margin-top: 5px;">
                        <i class="qq-upload-spinner-selector fa fa-spinner fa-spin"></i> 
                        <strong role="status" class="qq-upload-status-text-selector"></strong>
                    </div>
                    <div class="text-center" style="margin-top: 5px;">
                        <button type="button" class="qq-upload-cancel-selector btn btn-default btn-sm"><i class="fa fa-ban"></i> Cancelar</button>
                        <button type="button" class="qq-upload-retry-selector btn btn-warning btn-sm"><i class="fa fa-refresh"></i> Reenviar</button>
                        <button type="button" class="qq-upload-delete-selector btn btn-danger btn-sm"><i class="fa fa-times"></i> Excluir</button>
                    </div>
                </li>
            </ul>
            
            <dialog class="qq-alert-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">Close</button>
                </div>
            </dialog>

            <dialog class="qq-confirm-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">No</button>
                    <button type="button" class="qq-ok-button-selector">Yes</button>
                </div>
            </dialog>

            <dialog class="qq-prompt-dialog-selector">
                <div class="qq-dialog-message-selector"></div>
                <input type="text">
                <div class="qq-dialog-buttons">
                    <button type="button" class="qq-cancel-button-selector">Cancel</button>
                    <button type="button" class="qq-ok-button-selector">Ok</button>
                </div>
            </dialog>
        </div>
    </script>
{% endif %}